{
  "$schema": "https://www.schemastore.org/tsconfig",
  "display": "Node 24",
  "_version": "24.0.0",

  "compilerOptions": {
    "rewriteRelativeImportExtensions": true,
    "lib": [
      "DOM",
      "es2024",
      "ESNext.Array",
      "ESNext.Collection",
      "ESNext.Iterator",
      "ESNext.Promise",
    ],
    "module": "nodenext",
    "target": "es2024",
    "noImplicitAny": true,

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
  },
}
